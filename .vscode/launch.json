    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387 
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rust'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=renamer",
                    "--package=renamer"
                ],
                "filter": {
                    "name": "renamer",
                    "kind": "bin"
                }
            },
            "args": ["-d", "testdir", "-t"],
            "cwd": "${workspaceFolder}"
        },
    ]
}